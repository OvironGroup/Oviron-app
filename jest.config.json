{
  "moduleDirectories": ["node_modules"],
  "collectCoverageFrom": [
    "src/**/*.{tsx}",
    "!src/**/*.spec.{js,jsx}",
    "!**/node_modules/**"
  ],
	"transform": {},
  "coverageDirectory": "coverage/",
  "coverageReporters": ["html", "lcov"],
  "testURL": "http://localhost",
  "testMatch": ["**/?(*.)+(test).[jt]s?(x)"],
  "clearMocks": true,
  "restoreMocks": true,
  "resetMocks": true
}
